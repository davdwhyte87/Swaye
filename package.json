{
  "name": "pie_music",
  "version": "1.0.0",
  "description": "An api written in node js for online streaming service",
  "main": "server.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
